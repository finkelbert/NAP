@article{Llanos:TheJournalOfTheAcousticalSocietyOf:2015,
	author = {Llanos, Fernando  and Alexander, Joshua M.  and Stilp, Christian E.},
	title = {Shannon entropy predicts the sonority status of natural classes in English},
	journal = {The Journal of the Acoustical Society of America},
	uuid = {4B7F1A33-82C0-4637-9AD2-D10002E789D7},
	volume = {137},
	number = {4},
	pages = {2305},
