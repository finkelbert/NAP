	sentelink = {file:///Users/aviadalbert/Documents/Papers.sente6lib/Contents/Attachments/Yavas%CC%A7%2C%20M/2013/What%20explains%20the%20reductions%20i.pdf,Sente,},
	url = {},
	keywords = {Sonority;s-clusters;distinctive features;language acquisition;sonority;phonology;clusters},
	tags = {Sonority;s-clusters},
	Web_data_source = {Common},
	Citation_identifier = {Yavaş 2013},
	publicationStatus = {Published},
	abstract = {This study is a cross-linguistic examination of the patterns of reduction of two-member /s/-clusters. Data from six languages (English, Dutch, Norwegian, Croatian, Hebrew, and Polish) gathered from the productions of typically developing children and from children with phonological disorders are analyzed. While the languages belong to three different families, the results point to a universal tendency. Specifically, in the reductions of “s+stop” and “s+nasal” targets, the preference is the retention of C2, whereas in /sl/ and /sw/ targets, the clear pattern is in favor of the retention of C1. These patterns suggest that the continuancy of C2, not sonority, is the determining factor; C2 prevailed in “/s/+[-continuant]” targets, whereas C1 is retained in “/s/+[+continuant]” targets.}
}
