	Tags = {History;Jewism;Genetics},
	Title = {The Missing Link of Jewish European Ancestry: Contrasting the Rhineland and the Khazarian Hypotheses},
	Url = {http://gbe.oxfordjournals.org/content/5/1/61.abstract},
	Uuid = {0021B34A-3B7F-4DA9-8DEB-33198705FBF7},
