	ISSN = {ISSN-0142-7164},
	sentelink = {file:///Users/aviadalbert/Documents/Papers.sente6lib/Contents/Attachments/Hardison%2C%20DM/2010/Development%20of%20Perception%20of%20S.pdf,Sente,},
	url = {},
	keywords = {Scores;Language Proficiency;Auditory Perception;Second Language Learning;Undergraduate Students;English;Phonology;Speech Communication;Task Analysis;Language Tests;Japanese;Native Speakers},
	Medium_consulted = {Web},
	ERIC_number = {EJ875117},
	Web_data_source = {ERIC},
	Citation_identifier = {Hardison 2010},
	publicationStatus = {Published},
	abstract = {This study explored the second language perceptual accuracy of Japanese geminates (moraic units) by English-speaking learners at three proficiency levels: beginner (28), low-intermediate (42), and advanced (15). Stimuli included singleton and geminate /t/, /k/, /s/ followed by /a/ or /u/ produced by a native speaker in isolated words and carrier sentences. Results of a forced-choice identification task revealed a significant complex interaction involving all factors. Simple effects tests indicated scores for geminate /s/ followed by /u/ in carrier sentences were significantly lower across proficiency levels. The singleton-geminate contrast for /t, k/ was significant for beginning learners, marginal for low-intermediate, and nonsignificant for advanced. Duration differences did not account for all findings. Greater consonant-vowel sonority difference facilitated perception, enhancing mora boundaries for speech segmentation. (Contains 5 figures, 1 table and 6 notes.)}
}
