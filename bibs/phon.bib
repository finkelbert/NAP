@article{Grossman:JSpeechLangHearRes:2010,
	author = {Grossman, Ruth B.  and Bemis, Rhyannon H.  and Plesa Skwerer, Daniela  and Tager-Flusberg, Helen},
	title = {Lexical and affective prosody in children with high-functioning autism.},
	journal = {J Speech Lang Hear Res},
	uuid = {059AF4BB-E559-4162-8743-ED5D9E1CC9FE},
	volume = {53},
	number = {3},
	pages = {778-93},
	organization = {University of Massachusetts Medical School Shriver Center, Waltham, MY, USA. ruth\_grossman\@emerson.edu},
	address = {United States},
	month = {6},
	year = {2010},
	doi = {10.1044/1092-4388(2009/08-0127)},
	ISSN = {1558-9102},
	US_NLM_ID = {9705610},
	PII = {53/3/778},
	pubmedid = {20530388},
	PMCID = {3538164},
	sentelink = {file:///Users/aviadalbert/Documents/Papers.sente6lib/Contents/Attachments/Grossman%2C%20RB/2010/Lexical%20and%20affective%20prosody.webarchive,Sente,Web Page Archived 07 Aug 2018 15:32:38},
	sentelink = {file:///Users/aviadalbert/Documents/Papers.sente6lib/Contents/Attachments/Grossman%2C%20RB/2010/Lexical%20and%20affective%20prosody.pdf,Sente,},
	url = {},
	keywords = {research support, n.i.h., extramural;Linguistics;Psycholinguistics;Speech Production Measurement;ASD;Affect;Speech Perception;Language;Adolescent;Humans;Analysis of Variance;research support, non-u.s. gov't;Emotions;Child Language;Language Tests;Speech Acoustics;Child;Autistic Disorder},
	tags = {ASD},
	Citation_identifier = {Grossman 2010},
	publicationStatus = {Published},
	abstract = {PURPOSE: To investigate the perception and production of lexical stress and processing of affective prosody in adolescents with high-functioning autism (HFA). We hypothesized preserved processing of lexical and affective prosody but atypical lexical prosody production.
METHOD: Sixteen children with HFA and 15 typically developing (TD) peers participated in 3 experiments that examined the following: (a) perception of affective prosody (Experiment 1), (b) lexical stress perception (Experiment 2), and (c) lexical stress production (Experiment 3). In Experiment 1, participants labeled sad, happy, and neutral spoken sentences that were low-pass filtered, to eliminate verbal content. In Experiment 2, participants disambiguated word meanings based on lexical stress (HOTdog vs. hot DOG). In Experiment 3, participants produced these words in a sentence completion task. Productions were analyzed with acoustic measures.
RESULTS: Accuracy levels showed no group differences. Participants with HFA could determine affect from filtered sentences and disambiguate words on the basis of lexical stress. They produced appropriately differentiated lexical stress patterns but demonstrated atypically long productions, indicating reduced ability in natural prosody production.
CONCLUSIONS: Children with HFA were as capable as their TD peers in receptive tasks of lexical stress and affective prosody. Prosody productions were atypically long, despite accurate differentiation of lexical stress patterns. Future research should use larger samples and spontaneous versus elicited productions}
}
@article{Diehl:BrainLang:2008,
	author = {Diehl, Joshua J.  and Bennetto, Loisa  and Watson, Duane  and Gunlogson, Christine  and McDonough, Joyce},
